  "version": "1.0.0",
  "scripts": {
    "start": "gulp clean && tsc && concurrently \"tsc -w\" \"node index.js\"",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
